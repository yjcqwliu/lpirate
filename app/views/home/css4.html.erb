#sub-nav {
	PADDING-RIGHT: 10px; PADDING-LEFT: 10px; PADDING-BOTTOM: 0px; OVERFLOW: hidden; PADDING-TOP: 14px; BORDER-BOTTOM: #ccc 1px solid; ZOOM: 1;
}
#sub-nav UL {
	PADDING-LEFT: 2px; OVERFLOW: auto; ZOOM: 1; LIST-STYLE-TYPE: none;
}
#sub-nav UL LI {
	FLOAT: left; MARGIN-LEFT: -1px;
}
#sub-nav UL LI A {
	BORDER-RIGHT: #898989 1px solid; PADDING-RIGHT: 1em; BORDER-TOP: #898989 1px solid; MIN-WIDTH: 60px; DISPLAY: block; PADDING-LEFT: 1em; BACKGROUND:  #f1f1f1 repeat-x; PADDING-BOTTOM: 3px; BORDER-LEFT: #898989 1px solid; COLOR: #333; PADDING-TOP: 3px; BORDER-BOTTOM: #898989 0px solid; TEXT-ALIGN: center;
}



#sub-nav .selected {
	Z-INDEX: 99; POSITION: relative;
}
#sub-nav .selected A {
	BORDER-RIGHT: #5973a9 1px solid; BORDER-TOP: #3b5888 1px solid; BACKGROUND: #6d84b4; BORDER-LEFT: #5973a9 1px solid; COLOR: #fff; BORDER-BOTTOM: #3b5888 0px solid;
}
#sub-nav .snav {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; FLOAT: left; PADDING-BOTTOM: 5px; PADDING-TOP: 0px;
}
#sub-nav .pagerpro-title {
	PADDING-RIGHT: 0px; PADDING-LEFT: 0px; FLOAT: left; PADDING-BOTTOM: 5px; PADDING-TOP: 0px;
}
